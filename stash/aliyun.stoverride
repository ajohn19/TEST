name: |-
  aliyun
desc: |-
  aliyun is automatically converted by LEVI SCRIPT; if not available plz use Script-Hub.

http:

  mitm:
    - "auth.alipan.com"
    - "auth.aliyundrive.com"

  script:  
- match: "^https:\/\/(auth|aliyundrive)\.alipan\.com\/v2\/account\/token"
    type: http-request
    require-body: true
    max-size: -1
    timeout: 60


script-providers:
  "aliyun_62":
    url: undefined
    interval: 86400

