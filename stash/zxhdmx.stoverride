name: |-
  zxhdmx
desc: |-
  zxhdmx is automatically converted by LEVI SCRIPT; if not available plz use Script-Hub.

http:

  mitm:
    - "api.revenuecat.com"

  script:  - match: "^https?:\/\/api\.revenuecat\.com\/v1\/(subscribers\/[^\/]+$|receipts$)"
    type: http-response
    script-path: "https://raw.githubusercontent.com/Yu9191/Rewrite/main/zxhdmx.js"
    require-body: true
    max-size: -1
    timeout: 60

